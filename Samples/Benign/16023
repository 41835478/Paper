
  $(function () {
      $("#start_date_top").datepick();
      $("#start_date_icon_top").click(function () {
          $("#start_date_top").datepick("show");
      });
      $("#end_date_top").datepick();
      $("#end_date_icon_top").click(function () {
          $("#end_date_top").datepick("show");
      });
  });

