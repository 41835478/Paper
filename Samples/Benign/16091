
  var contactUrl = "/lb/userRole/1171";
  var homeUrl = "/lb/iList/1171";
  var displayIcon = new Array("false", "false", "false", "false", "false");
  function flipMode(position) {
      if (displayIcon[position] == "true") {
          document.getElementById("thing_" + position).className = "place-off-screen";
          displayIcon[position] = "false";
      } else {
          document.getElementById("thing_" + position).className = "place-on-screen";
          displayIcon[position] = "true";
      }
  }

