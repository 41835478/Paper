
  function abrirSubMenuCol3(seccionMenuCol3) {
      var objUl = document.getElementById("idGrupoMenuCol3_" + seccionMenuCol3);
      if (objUl) {
          if (objUl.style.display != "block") {
              objUl.style.display = "block";
              objUl.parentNode.className = "activa";
          } else {
              objUl.style.display = "none";
              objUl.parentNode.className = "";
          }
      }
  }
  var seccionActual = "estilo";
  if (seccionActual) {
      abrirSubMenuCol3(seccionActual);
  }

