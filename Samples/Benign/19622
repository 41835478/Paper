
  function jalbumIeDownload(url) {
      window.open(url, "jalbumdownload", "width=10,height=10,top=0,left=0,menubar=no,location=no,resizable=no,scrollbars=no,status=no,directories=no");
      location.href = "http://jalbum.net/en/downloadmirror/os/Windows/url/" + encodeURIComponent(encodeURIComponent(url));
  }

