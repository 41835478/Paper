
  DISQUS.define("web.urls", function () {
      return {twitter: "https://disqus.com/_ax/twitter/begin/", google: "https://disqus.com/_ax/google/begin/", facebook: "https://disqus.com/_ax/facebook/begin/", dashboard: "http://disqus.com/dashboard/"};
  });
  $(document).ready(function () {
      var usernameInput = $("input[name=username]");
      if (usernameInput[0].value) {
          $("input[name=password]").focus();
      } else {
          usernameInput.focus();
      }
  });

