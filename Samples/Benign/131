
  var ranimg = ["header16_a.jpg", "header16_b.jpg", "header16_c.jpg", "header16_d.jpg"], mai = Math.floor(ranimg.length * Math.random()), header = document.getElementById("header"), headerStyle = header.style, headerFilePath = "http://stat100.ameba.jp/p_skin/official_160/img/";
  ranimg = ranimg[Math.floor(mai)];
  headerStyle.background = "url(" + headerFilePath + ranimg + ") no-repeat";

