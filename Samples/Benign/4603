
  google.load("visualization", "1", {packages: ["geochart"]});
  google.load("visualization", "1", {packages: ["corechart"]});
  google.setOnLoadCallback(function () {
      $(document).ready(function () {
          Util.LoadWidget("metrics/totalnumber", "wgtTotalNumber");
          Util.LoadWidget("metrics/californiamap", "wgtCaliforniaMap");
          Util.LoadWidget("metrics/searchterms", "wgtSearchTerms");
          Util.LoadWidget("metrics/columnchart", "wgtBarActivity");
          Util.LoadWidget("metrics/appdownloads", "wgtAppDownloads");
          Util.LoadWidget("metrics/govtthisweek", "wgtGovtThisWeek");
          Util.LoadWidget("metrics/mobiletraffic", "wgtMobileTraffic");
          Util.LoadWidget("metrics/socialmedia", "wgtSocialMedia");
          Util.LoadWidget("socialmedia/twitter", "wgtTwitter");
          Util.LoadWidget("socialmedia/youtube", "wgtYoutube");
      });
  });

